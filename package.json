{
  "name": "tt",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@farfetched/core": "0.11.1",
    "@pbe/react-yandex-maps": "^1.2.5",
    "@types/react": "^18.3.6",
    "@types/react-dom": "^18.3.0",
    "@types/react-virtualized": "^9.21.30",
    "antd": "^5.20.6",
    "axios": "^1.7.7",
    "cypress": "^13.14.2",
    "dayjs": "^1.11.13",
    "effector": "^23.2.2",
    "effector-forms": "2.0.0-next.0",
    "effector-react": "^23.2.1",
    "effector-storage": "^7.1.0",
    "file-saver": "^2.0.5",
    "formik": "^2.4.6",
    "imask": "^7.6.1",
    "lint-staged": "^15.2.10",
    "lodash": "^4.17.21",
    "patronum": "^2.2.0",
    "query-string": "6.13.8",
    "react": "^18.3.1",
    "react-bootstrap-icons": "^1.11.4",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-router-dom": "^6.26.2",
    "react-to-print": "^2.15.1",
    "react-virtualized": "^9.22.5",
    "string-to-color": "^2.2.2",
    "styled-components": "5.2.0",
    "swagger-typescript-api": "^13.0.22",
    "typescript": "^5.6.2",
    "use-debounce": "^10.0.3",
    "victory": "^37.1.1",
    "yandex-maps": "^0.0.1",
    "yup": "0.29.3"
    
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "updateTypes": "npx swagger-typescript-api -p https://stage.k8s.transparent-technology.ru/openapi/site/ttm_api.json -o ./src/api -n types.ts && npx swagger-typescript-api -p https://stage.k8s.transparent-technology.ru/openapi/mvitu/ttm_api.json -o ./src/api -n mvitu.types.ts",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cypress:open": "cypress open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.{js,jsx,ts,tsx}"
        ],
        "rules": {
          "no-console": "warn"
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write",
      "eslint --max-warnings 0"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/helper-define-map": "^7.13.12",
    "@storybook/addon-actions": "^7.6.6",
    "@storybook/addon-essentials": "^7.6.6",
    "@storybook/addon-interactions": "^7.6.6",
    "@storybook/addon-links": "^7.6.6",
    "@storybook/node-logger": "^7.6.6",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^7.6.6",
    "@storybook/react-webpack5": "^7.6.6",
    "@storybook/testing-library": "^0.2.2",
    "@types/file-saver": "^2.0.5",
    "@types/lodash": "^4.14.191",
    "@types/styled-components": "^5.1.7",
    "@types/yandex-maps": "^2.1.35",
    "@types/yup": "^0.29.11",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-plugin-named-exports-order": "^0.0.2",
    "esbuild-plugin-react-virtualized": "^1.0.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.3",
    "prop-types": "^15.8.1",
    "sass": "^1.60.0",
    "storybook": "^7.6.6",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "vite": "^5.2.11",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "webpack": "^5.75.0"
  }
}
